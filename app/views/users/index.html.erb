  <div class="container">
      <div class="row">
      <%=render 'books/user_info'%>
        <div class="col-md-8 offset-md-1">
        <h1>Users</h1>
        <table class="table">
          <thead>
            <tr>
              <th scope="col">image</th>
              <th scope="col">name</th>
            </tr>
          </thead>
          <tbody>
          <% @users.each do |user| %>
          <tr>
            <td><%= attachment_image_tag user,:profile_image, :fill, 10, 10, fallback: "no_image.jpg"%></td>
            <td><%= user.name %></td>
            <td><%=link_to "Show", user_path(user)%></td>
          </tr>
          <% end %>
        </tbody>
       </table>
       </div>
     </div>
 </div>
